{
  "$id": "https://openactive.io/socialrx/person.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Person",
  "type": "object",
  "required": [ "name", "@type" ],
  "properties": {
    "@type": {
      "type": "string",
      "const": "Person"
    },
    "name": {
      "type": "string",
      "description": "The name of the Person."
    }
  }
}
