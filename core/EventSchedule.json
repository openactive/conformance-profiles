{
  "$id": "https://openactive.io/core/eventschedule.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EventSchedule",
  "type": "object",
  "required": [ "startDate", "repeatFrequency"],
  "properties": {
    "startDate": {
      "type": "string",
      "maxLength": 20
    },
    "repeatFrequency": {
      "type": "string",
      "maxLength": 10
    },
    "anyOf":{
      "endDate": {
        "type": "string",
        "maxLength": 20
      },
      "duration": {
        "type": "string",
        "maxLength": 10
      }
    }
  }
}
