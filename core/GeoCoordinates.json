{
  "$id": "https://openactive.io/core/geocoordinates.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GeoCoordinates",
  "type": "object",
  "required": [ "latitude", "longitude", "@type" ],
  "properties": {
    "@type": {
      "type": "string",
      "const": "GeoCoordinates"
    },
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    }
  }
}
