{
  "$id": "https://openactive.io/core/offer.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Offer",
  "type": "object",
  "required": [ "price", "@type" ],
  "properties": {
    "@type": {
      "type": "string",
      "const": "Offer"
    },
    "price": {
      "type": "number",
      "description": "The price of attending the Event."
    }
  }
}
