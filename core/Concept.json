{
  "$id": "https://openactive.io/core/concept.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Concept",
  "type": "object",
  "required": [ "name", "@type" ],
  "properties": {
    "@type": {
      "type": "string",
      "const": "Concept"
    },
    "name": {
      "type": "string",
      "description": "The name of the Concept (e.g., of an activity or difficulty level)."
    }
  }
}
